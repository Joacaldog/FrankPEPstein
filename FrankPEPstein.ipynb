      "source": [
        "#@title Prepare Inputs & Patch Scripts\n",
        "import Bio.PDB\n",
        "\n",
        "selected_pocket = pocket_dropdown.value\n",
        "pocket_abs_path = os.path.abspath(os.path.join(pockets_dir, selected_pocket))\n",
        "print(f\"Selected Pocket: {pocket_abs_path}\")\n",
        "\n",
        "# Calculate Box Center and Size from Pocket PDB\n",
        "def get_pocket_box(pdb_file):\n",
        "    parser = Bio.PDB.PDBParser(QUIET=True)\n",
        "    structure = parser.get_structure(\"pocket\", pdb_file)\n",
        "    coords = []\n",
        "    for atom in structure.get_atoms():\n",
        "        coords.append(atom.get_coord())\n",
        "\n",
        "    min_coord = [min([c[i] for c in coords]) for i in range(3)]\n",
        "    max_coord = [max([c[i] for c in coords]) for i in range(3)]\n",
        "\n",
        "    center = [(min_coord[i] + max_coord[i]) / 2 for i in range(3)]\n",
        "    size = [(max_coord[i] - min_coord[i]) + 5.0 for i in range(3)] # Add buffer\n",
        "    return center, size\n",
        "\n",
        "center, size = get_pocket_box(pocket_abs_path)\n",
        "print(f\"Box Center: {center}\\nBox Size: {size}\")\n",
        "\n",
        "# Identify paths\n",
        "notebook_dir = os.getcwd()\n",
        "repo_dir = os.path.join(notebook_dir, \"FrankPEPstein\")\n",
        "scripts_dir = os.path.join(repo_dir, \"scripts\")\n",
        "utilities_dir = os.path.join(repo_dir, \"utilities\")\n",
        "db_dir = os.path.join(repo_dir, \"DB\")\n",
        "\n",
        "# Paths to replace in scripts\n",
        "path_replacements = {\n",
        "    \"/home/jgutierrez/scripts/\": f\"{scripts_dir}/\",\n",
        "    \"/home/jgutierrez/utilities/./vina_1.2.4_linux_x86_64\": \"vina\", # Assuming installed via conda\n",
        "    \"/home/jgutierrez/scripts/reduce_wwPDB_het_dict.txt\": f\"{db_dir}/reduce_wwPDB_het_dict.txt\",\n",
        "    \"/home/jgutierrez/FrankPEPstein/filtered_DB_P5-15_R30_id10\": f\"{db_dir}/minipockets_surface80_winsize3_size3_curated-db\" # Check extracted name\n",
        "}\n",
        "\n",
        "print(\"Patching scripts...\")\n",
        "for script_name in os.listdir(scripts_dir):\n",
        "    if script_name.endswith(\".py\"):\n",
        "        full_path = os.path.join(scripts_dir, script_name)\n",
        "        with open(full_path, 'r') as f:\n",
        "            content = f.read()\n",
        "\n",
        "        for old, new in path_replacements.items():\n",
        "            content = content.replace(old, new)\n",
        "\n",
        "        # Additional patches for command calls\n",
        "        # Use 'vina' from path instead of absolute path\n",
        "        content = content.replace(\"vina_1.2.4_linux_x86_64\", \"vina\")\n",
        "\n",
        "        with open(full_path, 'w') as f:\n",
        "            f.write(content)\n",
        "        print(f\"Patched {script_name}\")\n"
      ],
