npts 40 40 40                        # num.grid points in xyz
gridfld H_receptor.maps.fld          # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C NA OA N SA HD     # receptor atom types
ligand_types NA C OA                 # ligand atom types
receptor H_receptor.pdbqt            # macromolecule
gridcenter -20.966 -27.500 16.718    # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map H_receptor.NA.map                # atom-specific affinity map
map H_receptor.C.map                 # atom-specific affinity map
map H_receptor.OA.map                # atom-specific affinity map
elecmap H_receptor.e.map             # electrostatic potential map
dsolvmap H_receptor.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
